{
  "id": "braids",
  "name": "Braids",
  "abbrev": "BR",
  "version": "0.2.0",
  "description": "Macro oscillator with 47 synthesis algorithms, based on Mutable Instruments Braids",
  "author": "Emilie Gillet (port: charlesvestal)",
  "license": "MIT",
  "dsp": "dsp.so",
  "api_version": 2,
  "standalone": false,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": true,
    "midi_out": false,
    "aftertouch": false,
    "chainable": true,
    "component_type": "sound_generator",
    "chain_params": [
      {"key": "engine", "name": "Algorithm", "type": "enum", "default": "CSAW", "options": ["CSAW","MORPH","/\\-_","SINE^","BUZZ","SQR<","SAW<","SQsync","SWsync","3xSAW","3xSQR","3xTRI","3xSIN","3xRNG","SWARM","COMB","TOY","ZLPF","ZPKF","ZBPF","ZHPF","VOSIM","VOWL","V.FOF","HARM","FM","FBFM","WTFM","PLUK","BOWD","BLOW","FLUT","BELL","DRUM","KICK","CYMB","SNAR","WTBL","WMAP","WLIN","WTx4","NOIS","TWNQ","CLKN","GRN","PART","QPSK"]},
      {"key": "timbre", "name": "Timbre", "type": "float", "min": 0.0, "max": 1.0, "default": 0.5, "step": 0.02},
      {"key": "color", "name": "Color", "type": "float", "min": 0.0, "max": 1.0, "default": 0.5, "step": 0.02},
      {"key": "attack", "name": "Attack", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.02},
      {"key": "decay", "name": "Decay", "type": "float", "min": 0.0, "max": 1.0, "default": 0.5, "step": 0.02},
      {"key": "sustain", "name": "Sustain", "type": "float", "min": 0.0, "max": 1.0, "default": 1.0, "step": 0.02},
      {"key": "release", "name": "Release", "type": "float", "min": 0.0, "max": 1.0, "default": 0.3, "step": 0.02},
      {"key": "fm", "name": "FM", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.02},
      {"key": "cutoff", "name": "Cutoff", "type": "float", "min": 0.0, "max": 1.0, "default": 1.0, "step": 0.02},
      {"key": "resonance", "name": "Resonance", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.02},
      {"key": "filt_env", "name": "Filt Env", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.02},
      {"key": "f_attack", "name": "F.Attack", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.02},
      {"key": "f_decay", "name": "F.Decay", "type": "float", "min": 0.0, "max": 1.0, "default": 0.3, "step": 0.02},
      {"key": "f_sustain", "name": "F.Sustain", "type": "float", "min": 0.0, "max": 1.0, "default": 0.0, "step": 0.02},
      {"key": "f_release", "name": "F.Release", "type": "float", "min": 0.0, "max": 1.0, "default": 0.3, "step": 0.02},
      {"key": "volume", "name": "Volume", "type": "float", "min": 0.0, "max": 1.0, "default": 0.7, "step": 0.02},
      {"key": "octave_transpose", "name": "Octave", "type": "int", "min": -3, "max": 3, "default": 0}
    ]
  }
}
